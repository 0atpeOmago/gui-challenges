{
  "hosting": {
    "public": "/",
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [
      {
        "source": "/stories/**",
        "destination": "/stories/dist/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}